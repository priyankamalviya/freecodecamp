function largestOfFour(arr) {
  // You can do this!
  var subarr=[0,0,0,0];
  for(var i=0;i<arr.length;i++){
    var large=0;
    for(var j=0;j<4;j++){
    if(arr[i][j]>large){
      large=arr[i][j];
    }
      subarr[i]=large;
  }
  }
  return subarr;
}

largestOfFour([[13, 27, 18, 26], [4, 5, 1, 3], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
